		<script type="text/javascript">
			var equalHeightRunOnce = false;

			function equalHeight(force) {
				if ( force || !equalHeightRunOnce || $( "div#center div.right" ).length ) {
					$( "div#center div.middle .main" ).height( 'auto' );
					var left_height = $( "div#center div.left" ).height();
					var leftalt_height = $( "div#center div#ie6LeftProjectAlternative" ).height();
					var center_height = $( "div#center div.middle" ).height();
					var right_height = $( "div#center div.right" ).height();
					var max_height = center_height;
					if ( left_height > max_height ) max_height = left_height;
					if ( leftalt_height > max_height ) max_height = leftalt_height;
					if ( right_height > max_height ) max_height = right_height;
					if ( max_height > center_height ) $( "div#center div.middle .main" ).height( max_height + "px" );
				}
				equalHeightRunOnce = true;

				// ie8 and ie9 don't re-render the screen after reducing the center div height.  
				// hide and show the footer to get it to re-draw in the correct position.
				$("div#footer").hide();
				$("div#footer").show();

			}

			$( document ).ready( equalHeight );

			$( window ).resize( equalHeight );

			function fixPngButtonsForIe6() {
				if ( $( "html.ie6" ).length ) {
					var selectors = [
								"input#globalSearchButton", // global
								"input#findFormButton", // global
								"img.informationIcon", // global
								"img#moreForumsButton", // home page
								"img#moreFilesButton", // home page
								"img#moreToolsButton", // home page
								"img#moreNewsButton", // home page
								"img#downloadNowImage", // project summary pages
								"img#seeAllFilesImage", // project summary pages
								"img.downloadNowImage", // search results page (and possibly others)
								"input.adjustSearchButton" // search results page (and possibly others)
							];
					for ( var i = 0; selector = selectors[i]; ++i < selectors.length ) {
						var button = $( selector );
						if ( button.length ) button.attr( "src", button.attr( "src" ).replace( '.24.png', '.8.png' ) );
					}
				}
			}

			$( document ).ready( fixPngButtonsForIe6 );

			function activateImage( it ) {
				if ( !it.isActive ) {
					it.src = it.src.replace( /\.8\.png$/, "-8.png" )
										.replace( /\.24\.png$/, "-24.png" )
										.replace( /(\/.+)(\.png|\.jpg|\.jpeg|\.gif)$/, "$1-hover$2" )
										.replace( /-24-hover\.png$/, "-hover.24.png" )
										.replace( /-8-hover\.png$/, "-hover.8.png" )
					it.isActive = true;
				}
			}

			function deactivateImage( it ) {
				if ( it.isActive ) {
					it.src = it.src.replace( /-hover\./, "." );
					it.isActive = false;
				}
			}

			function addButtonRollovers( scopingSelector ) {
				if ( typeof scopingSelector != 'string' ) scopingSelector = '';
				if ( !scopingSelector.match( /^$|.*\s$/ ) ) scopingSelector += ' ';
				$( scopingSelector + ".rollover" ).hover(
					function() { activateImage( this ); },
					function() { deactivateImage( this ); }
				);
				$( scopingSelector + "input.rollover" ).focus( function() { activateImage( this ); } );
				$( scopingSelector + "input.rollover" ).blur( function() { deactivateImage( this ); } );
				$( scopingSelector + "a.imageLink.withRollover" ).focus( function() { activateImage( this.firstChild ); } );
				$( scopingSelector + "a.imageLink.withRollover" ).blur( function() { deactivateImage( this.firstChild ); } );
			}

			$( document ).ready( addButtonRollovers );

			function setUpGlobalMenus() {
				$( "ol#globalNavigation > li ol" ).css( { display: "none" } ); // Opera fix...
				$( "ol#globalNavigation li" ).hover(
					function() {
						if ( $( "html.ie6" ).length ) $( "#globalTypeOfSearch" ).css( { visibility: "hidden" } );
						try { $( this ).find( "ol:first" ).css( { display: "block" } ); } catch (ex) {}
						$( this ).find( "ol:first" ).css( { visibility: "visible" } );
					},
					function() {
						if ( $( "html.ie6" ).length ) $( "#globalTypeOfSearch" ).css( { visibility: "visible" } );
						try { $( this ).find( "ol:first" ).css( { display: "none" } ); } catch (ex) {}
						$( this ).find( "ol:first" ).css( { visibility: "hidden" } );
					}
				);
				$( "ol#globalNavigation li" ).focus(
					function() {
						if ( $( "html.ie6" ).length ) $( "#globalTypeOfSearch" ).css( { visibility: "hidden" } );
						try { $( this ).find( "ol:first" ).css( { display: "block" } ); } catch (ex) {}
						$( this ).find( "ol:first" ).css( { visibility: "visible" } );
					}
				);
				$( "ol#globalNavigation li" ).blur(
					function() {
						if ( $( "html.ie6" ).length ) $( "#globalTypeOfSearch" ).css( { visibility: "visible" } );
						try { $( this ).find( "ol:first" ).css( { display: "none" } ); } catch (ex) {}
						$( this ).find( "ol:first" ).css( { visibility: "hidden" } );
					}
				);
			}

			$( document ).ready( setUpGlobalMenus );
			function setUpResultTwisties() {
				$( "div.result" ).each(
					function() {
					  var name = $( this ).find( ".header span.name" ).html();
            $( this ).find( ".header span.name" ).replaceWith( '<a class="twistie" href="#" title="Open/Close"><span>&#9660;</span> ' + name + '</a>' );
            $( this ).find( ".header a.twistie" ).click(
    					function() {
    					  var result = $( this ).parent().parent();
    						var isOpen = result.hasClass( "closed" ) ? false : true;
    						isOpen ? result.addClass( "closed" ) : result.removeClass( "closed" );
    						$( this ).find( "span" ).html( isOpen ? "&#9658;" : "&#9660;" );
    						result.find( "div.body" ).css( { display: ( isOpen ? "none" : "block" ) } );
    						equalHeight(true);
    						return false;
    					}
    				);
					}
				);
			}

			$( document ).ready( setUpResultTwisties );

			function setUpShowAllSpecificationsLinks() {
				$( "div.specifications.more" ).each(
					function() {
    				$( this ).toggle(false);
					  var previous = $( this ).prev();
            previous.find( "div" ).html( previous.find( "div" ).html() + ' <a class="ephemeralLink" href="#">(Show all specifications)</a>' );
            previous.find( "a.ephemeralLink" ).click(
    					function() {
    					  $( this ).parent().parent().next().toggle();
    					  $( this ).css( { display: "none" } );
    						equalHeight(true);
    						return false;
    					}
    				);
					}
				);
			}

			$( document ).ready( setUpShowAllSpecificationsLinks );

			var narrowingMenuTopListItemPadding = 0;

			function setUpNarrowingMenu() {
				try {
					narrowingMenuTopListItemPadding = parseInt( $( "#narrowingMenuContainer ol.menu li.top:first" ).css( 'padding-left' ) ) +
																						parseInt( $( "#narrowingMenuContainer ol.menu li.top:first" ).css( 'padding-right' ) );
				} catch (ex) {}
				$( "a.narrowingMenuCategory" ).each(
					function() {
						var isOpen = $( this ).hasClass( "closed" ) ? false : true;
						$( this ).find( "span.twistie" ).html( isOpen ? "&#9660;" : "&#9658;" );
						$( this ).next().css( { display: ( isOpen ? 'block' : 'none' ) } );
					}
				);
				$( "a.narrowingMenuCategory" ).click(
					function() {
						var isOpen = $( this ).hasClass( "closed" ) ? false : true;
						isOpen ? $( this ).addClass( "closed" ) : $( this ).removeClass( "closed" );
						$( this ).find( "span.twistie" ).html( isOpen ? "&#9658;" : "&#9660;" );
						$( this ).next().css( { display: ( isOpen ? 'none' : 'block' ) } );
						jiggleNarrowingMenu();
						equalHeight(true);
						return false;
					}
				);
			}

			$( document ).ready( setUpNarrowingMenu );

function debug(x) {
	$( "#debug" ).html( $( "#debug" ).html() + '<p>'+x+'</p>' );
}

			function jiggleNarrowingMenu() {
				$( "#narrowingMenu" ).css( { display: "none" } );
				$( "#narrowingMenu" ).css( { width: parseInt($( "#narrowingMenuContainer" ).width()) - 7 + "px" } );
				$( "#narrowingMenu" ).css( { display: "block" } );
				$( "#narrowingMenuContainer" ).css( { marginTop: parseInt($( "#pageTabs" ).height()) - 13 + "px" } );
				if ( ( $( "html.ie6" ).size() == 0 ) && ( $( "html.ie7" ).size() == 0 ) && 
					 $( "#pageTabs" ).offset() && $( "#narrowingMenuContainer" ).offset() ) {
					var targetwidth = $( "#pageTabs" ).offset().left -
														$( "#narrowingMenuContainer" ).offset().left -
														narrowingMenuTopListItemPadding -
														9;
					$( "#narrowingMenuContainer ol.menu li.top" ).width( targetwidth );
					var difference = $( "#pageTabs" ).offset().left -
													 $( "#narrowingMenuContainer" ).offset().left -
	 												 $( "#narrowingMenuContainer" ).width();
					if ( difference ) {
						$( "#narrowingMenuContainer ol.menu li.top" ).each( function() { $( this ).width( $( this ).width() + difference - 1 ); } );
					}
					difference = $( "#pageTabs" ).offset().left -
											 $( "#narrowingMenuContainer" ).offset().left -
	 										 $( "#narrowingMenuContainer" ).width();
					$( "#narrowingMenuContainer" ).width( $( "#pageTabs" ).offset().left - $( "#narrowingMenuContainer" ).offset().left );
				}
				$( "#narrowingMenuContainer" ).css( { visibility: "visible" } );
				equalHeight(true);
			}

			$( document ).ready( jiggleNarrowingMenu );

			$( window ).resize( jiggleNarrowingMenu );

		</script>
		<script>
			document.write( '<style>#narrowingMenuContainer { visibility: hidden; }</style>' ); // Hide the narrowing menu initially, but only when Javascript is enabled...
		</script>
		<script type="text/javascript">
			function admin_window(adminurl) {
				AdminWin = window.open(adminurl,'AdminWindow','scrollbars=yes,resizable=yes,toolbar=yes,height=400,width=400,top=2,left=2');
				AdminWin.focus();
			}

			function help_window(helpurl,options) {
				if (options == undefined) options='scrollbars=yes,resizable=yes,toolbar=no,height=400,width=400';
				HelpWin = window.open(helpurl,'HelpWindow',options);
				HelpWin.focus();
			}
		</script>
